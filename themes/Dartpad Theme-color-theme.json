{
  "name": "Dartpad Theme",
  "type": "dark",
  "colors": {
    "panel.border": "#203954",
    "panel.background": "#121A25",
    "panelTitle.activeBorder": "#203954",
    "panelTitle.activeForeground": "#FEFEFE",
    "panelTitle.inactiveForeground": "#808080",
    "terminal.background": "#12202F",
    "terminal.foreground": "#FEFEFE",
    "terminal.ansiBrightBlack": "#6272A4",
    "terminal.ansiBrightRed": "#FF6E6E",
    "terminal.ansiBrightGreen": "#69FF94",
    "terminal.ansiBrightYellow": "#FFFFA5",
    "terminal.ansiBrightBlue": "#D6ACFF",
    "terminal.ansiBrightMagenta": "#FF92DF",
    "terminal.ansiBrightCyan": "#A4FFFF",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBlack": "#21222C",
    "terminal.ansiRed": "#FF5555",
    "terminal.ansiGreen": "#50FA7B",
    "terminal.ansiYellow": "#F1FA8C",
    "terminal.ansiBlue": "#BD93F9",
    "terminal.ansiMagenta": "#FF79C6",
    "terminal.ansiCyan": "#8BE9FD",
    "terminal.ansiWhite": "#F8F8F2",
    "contrastBorder": "#203954",
    "focusBorder": "#203954",
    "foreground": "#B5C2C5",
    "selection.background": "#203954",
    "errorForeground": "#F7977A",
    "button.background": "#203954",
    "button.foreground": "#FEFEFE",
    "dropdown.background": "#121A25",
    "dropdown.border": "#203954",
    "dropdown.foreground": "#FEFEFE",
    "input.background": "#12202F",
    "input.foreground": "#B5C2C5",
    "input.border": "#12202F",
    "input.placeholderForeground": "#B5C2C5",
    "inputOption.activeBorder": "#46e209",
    "inputValidation.infoBorder": "#7EA7D8",
    "inputValidation.warningBorder": "#FFB86C",
    "inputValidation.errorBorder": "#F7977A",
    "badge.foreground": "#B5C2C5",
    "badge.background": "#121A25",
    "progressBar.background": "#B5C2C5",
    "list.activeSelectionBackground": "#203954",
    "list.activeSelectionForeground": "#FEFEFE",
    "list.dropBackground": "#203954",
    "list.focusBackground": "#B5C2C5",
    "list.highlightForeground": "#B5C2C5",
    "list.hoverBackground": "#203954",
    "list.inactiveSelectionBackground": "#203954",
    "list.warningForeground": "#46e209",
    "list.errorForeground": "#F7977A",
    "activityBarBadge.background": "#121A25",
    "activityBar.background": "#12202F",
    "activityBar.activeBackground": "#9ff99310",
    "activityBar.foreground": "#B5C2C5",
    "activityBar.inactiveForeground": "#808080",
    "activityBar.activeBorder": "#203954",
    "activityBarBadge.foreground": "#B5C2C5",
    "sideBarTitle.foreground": "#B5C2C5",
    "sideBar.background": "#121A25",
    "sideBar.border": "#12202F",
    "sideBarSectionHeader.background": "#12202F",
    "sideBarSectionHeader.border": "#808080",
    "editorGroup.border": "#203954",
    "editorGroupHeader.tabsBackground": "#121A25",
    "editorGroup.dropBackground": "#203954",
    "tab.activeForeground": "#FEFEFE",
    "tab.activeBorder": "#121A25",
    "tab.inactiveBackground": "#121A25",
    "tab.inactiveForeground": "#808080",
    "editor.background": "#12202F",
    "editor.foreground": "#B5C2C5",
    // Here
    // Green placeholder: 46e209
    "editorLineNumber.foreground": "#808080",
    "editor.selectionBackground": "#203954",
    "editor.selectionHighlightBackground": "#80808036",
    "editor.foldBackground": "#203954",
    "editor.wordHighlightBackground": "#808080",
    "editor.wordHighlightStrongBackground": "#80808036",
    "editor.findMatchBackground": "#203954",
    "editor.findMatchHighlightBackground": "#B5C2C540",
    "editor.findRangeHighlightBackground": "#46e209",
    "editor.hoverHighlightBackground": "#808080",
    "editor.lineHighlightBorder": "#203954",
    "editorLink.activeForeground": "#B5C2C5",
    "editor.rangeHighlightBackground": "#46e209",

    "window.activeBorder": "#121A25"
